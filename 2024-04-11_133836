#  10s+
# Tables
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_lang'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_lang`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_element'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_element`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_property'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_property`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_element_property'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_element_property`\G
# EXPLAIN /*!50100 PARTITIONS*/
SELECT DISTINCT BE.ID as ID,BE.NAME as NAME, FPV0.VALUE as PROPERTY_REGION_VALUE, FPV0.ID as PROPERTY_REGION_VALUE_ID
				FROM 
			b_iblock B
			INNER JOIN b_lang L ON B.LID=L.LID
			INNER JOIN b_iblock_element BE ON BE.IBLOCK_ID = B.ID
			LEFT JOIN b_iblock_property FP0 ON FP0.IBLOCK_ID = B.ID AND  FP0.CODE='REGION'
			LEFT JOIN b_iblock_property FP1 ON FP1.IBLOCK_ID = B.ID AND  FP1.CODE='CODE'
			LEFT JOIN b_iblock_element_property FPV0 ON FPV0.IBLOCK_PROPERTY_ID = FP0.ID AND FPV0.IBLOCK_ELEMENT_ID = BE.ID
			LEFT JOIN b_iblock_element_property FPV1 ON FPV1.IBLOCK_PROPERTY_ID = FP1.ID AND FPV1.IBLOCK_ELEMENT_ID = BE.ID
		
				WHERE 1=1 
			AND (
			
				((((BE.ACTIVE='Y'))))
				AND (
				
					(
					
						((((BE.NAME = 'Москва'))))
					)
					OR (
					
						((((BE.CODE = 'moskva'))))
					)
					OR (
					
						((((FPV1.VALUE = 'moskva'))))
					)
				)
				AND ((((BE.IBLOCK_ID = '26'))))
			)
			AND (((BE.WF_STATUS_ID=1 AND BE.WF_PARENT_ELEMENT_ID IS NULL)))
				
				 ORDER BY BE.ID asc\G

