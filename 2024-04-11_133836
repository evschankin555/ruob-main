	`wd_seo_url`.`SEO_DISABLE_SEARCHING` AS `SEO_DISABLE_SEARCHING`,
	`wd_seo_url`.`SEO_HIDE_FROM_SITEMAP` AS `SEO_HIDE_FROM_SITEMAP`,
	`wd_seo_url`.`SEO_SITEMAP_PRIORITY` AS `SEO_SITEMAP_PRIORITY`,
	`wd_seo_url`.`SEO_SITEMAP_CHANGEFREQ` AS `SEO_SITEMAP_CHANGEFREQ`,
	`wd_seo_url`.`HASH` AS `HASH`,
	`wd_seo_url`.`SESSION_ID` AS `SESSION_ID`,
	`wd_seo_url`.`EXTERNAL_ID` AS `EXTERNAL_ID`,
	`wd_seo_url`.`DATE_CREATED` AS `DATE_CREATED`,
	`wd_seo_url`.`DATE_MODIFIED` AS `DATE_MODIFIED`,
	`wd_seo_url`.`SEO_TEXT_1` AS `SEO_TEXT_1`,
	`wd_seo_url`.`SEO_TEXT_1_TYPE` AS `SEO_TEXT_1_TYPE`,
	`wd_seo_url`.`SEO_TEXT_2` AS `SEO_TEXT_2`,
	`wd_seo_url`.`SEO_TEXT_2_TYPE` AS `SEO_TEXT_2_TYPE`,
