#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_product`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_catalog_price'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_price`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_catalog_iblock'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_iblock`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_catalog_vat'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_vat`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_user'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_user`\G
# EXPLAIN /*!50100 PARTITIONS*/
SELECT  BE.ID as ID,BE.NAME as NAME,BE.DETAIL_PICTURE as DETAIL_PICTURE,DATE_FORMAT(BE.TIMESTAMP_X, '%d.%m.%Y %H:%i:%s') as TIMESTAMP_X,UNIX_TIMESTAMP(BE.TIMESTAMP_X) as TIMESTAMP_X_UNIX,BE.MODIFIED_BY as MODIFIED_BY,DATE_FORMAT(BE.DATE_CREATE, '%d.%m.%Y %H:%i:%s') as DATE_CREATE,UNIX_TIMESTAMP(BE.DATE_CREATE) as DATE_CREATE_UNIX,BE.CREATED_BY as CREATED_BY,BE.IBLOCK_ID as IBLOCK_ID,BE.IBLOCK_SECTION_ID as IBLOCK_SECTION_ID,BE.ACTIVE as ACTIVE,BE.ACTIVE_FROM as ACTIVE_FROM_X, case when EXTRACT(HOUR FROM BE.ACTIVE_FROM) > 0 OR EXTRACT(MINUTE FROM BE.ACTIVE_FROM) > 0 OR EXTRACT(SECOND FROM BE.ACTIVE_FROM) > 0 then DATE_FORMAT(BE.ACTIVE_FROM, '%d.%m.%Y %H:%i:%s') else DATE_FORMAT(BE.ACTIVE_FROM, '%d.%m.%Y') end as ACTIVE_FROM,case when EXTRACT(HOUR FROM BE.ACTIVE_TO) > 0 OR EXTRACT(MINUTE FROM BE.ACTIVE_TO) > 0 OR EXTRACT(SECOND FROM BE.ACTIVE_TO) > 0 then DATE_FORMAT(BE.ACTIVE_TO, '%d.%m.%Y %H:%i:%s') else DATE_FORMAT(BE.ACTIVE_TO, '%d.%m.%Y') end as ACTIVE_TO,case when EXTRACT(HOUR FROM BE.ACTIVE_FROM) > 0 OR EXTRACT(MINUTE FROM BE.ACTIVE_FROM) > 0 OR EXTRACT(SECOND FROM BE.ACTIVE_FROM) > 0 then DATE_FORMAT(BE.ACTIVE_FROM, '%d.%m.%Y %H:%i:%s') else DATE_FORMAT(BE.ACTIVE_FROM, '%d.%m.%Y') end as DATE_ACTIVE_FROM,case when EXTRACT(HOUR FROM BE.ACTIVE_TO) > 0 OR EXTRACT(MINUTE FROM BE.ACTIVE_TO) > 0 OR EXTRACT(SECOND FROM BE.ACTIVE_TO) > 0 then DATE_FORMAT(BE.ACTIVE_TO, '%d.%m.%Y %H:%i:%s') else DATE_FORMAT(BE.ACTIVE_TO, '%d.%m.%Y') end as DATE_ACTIVE_TO,BE.SORT as SORT,BE.PREVIEW_PICTURE as PREVIEW_PICTURE,BE.PREVIEW_TEXT as PREVIEW_TEXT,BE.PREVIEW_TEXT_TYPE as PREVIEW_TEXT_TYPE,BE.DETAIL_TEXT as DETAIL_TEXT,BE.DETAIL_TEXT_TYPE as DETAIL_TEXT_TYPE,BE.SEARCHABLE_CONTENT as SEARCHABLE_CONTENT,BE.WF_STATUS_ID as WF_STATUS_ID,BE.WF_PARENT_ELEMENT_ID as WF_PARENT_ELEMENT_ID,BE.WF_LAST_HISTORY_ID as WF_LAST_HISTORY_ID,BE.WF_NEW as WF_NEW,case when BE.WF_DATE_LOCK is null then 'green' when DATE_ADD(BE.WF_DATE_LOCK, INTERVAL 3600 SECOND) < NOW() then 'green' when BE.WF_LOCKED_BY = 0 then 'yellow' else 'red' end as LOCK_STA
					FROM 
			b_iblock B
			INNER JOIN b_lang L ON B.LID=L.LID
			INNER JOIN b_iblock_element BE ON BE.IBLOCK_ID = B.ID
			left join b_catalog_product as CAT_PR on (CAT_PR.ID = BE.ID)
			left join b_catalog_price as CAT_P1 on (CAT_P1.PRODUCT_ID = BE.ID and CAT_P1.CATALOG_GROUP_ID = 1)
			left join b_catalog_iblock as CAT_IB on ((CAT_PR.VAT_ID IS NULL or CAT_PR.VAT_ID = 0) and CAT_IB.IBLOCK_ID = BE.IBLOCK_ID)
			left join b_catalog_vat as CAT_VAT on (CAT_VAT.ID = CASE WHEN (CAT_PR.VAT_ID IS NULL OR CAT_PR.VAT_ID = 0) THEN CAT_IB.VAT_ID ELSE CAT_PR.VAT_ID END)
			left join b_catalog_price as PRC_FT on (PRC_FT.PRODUCT_ID = BE.ID)
			left join b_catalog_product as PRD on (PRD.ID = BE.ID)
			LEFT JOIN b_user U ON U.ID=BE.MODIFIED_BY
			LEFT JOIN b_user UL ON UL.ID=BE.WF_LOCKED_BY
			LEFT JOIN b_user UC ON UC.ID=BE.CREATED_BY
		
					WHERE 1=1 
			AND (
			
				((((BE.IBLOCK_ID = '14'))))
				AND ((((PRC_FT.PRICE > '0'))))
				AND ((((PRD.QUANTITY > '0'))))
			)
			AND (((BE.WF_STATUS_ID=1 AND BE.WF_PARENT_ELEMENT_ID IS NULL)))
					
					 ORDER BY CAT_PR.QUANTITY desc ,CAT_P1.PRICE_SCALE asc 
				 LIMIT 20 OFFSET 7820\G

# Query 2: 0.01 QPS, 0.01x concurrency, ID 0x64EDFA8B0FBBA28444E0B5D93EF4D18C at byte 128596990
# This item is included in the report because it matches --limit.
