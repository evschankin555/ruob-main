#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_section_element`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_section'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_section`\G
# EXPLAIN /*!50100 PARTITIONS*/
SELECT  BE.IBLOCK_ID as IBLOCK_ID,BE.ID as ID,B.DETAIL_PAGE_URL as DETAIL_PAGE_URL,BE.IBLOCK_SECTION_ID as IBLOCK_SECTION_ID,BE.DETAIL_PICTURE as DETAIL_PICTURE,BE.DETAIL_TEXT as DETAIL_TEXT,L.DIR as LANG_DIR,BE.CODE as CODE,BE.XML_ID as EXTERNAL_ID,B.IBLOCK_TYPE_ID as IBLOCK_TYPE_ID,B.CODE as IBLOCK_CODE,B.XML_ID as IBLOCK_EXTERNAL_ID,B.LID as LID,BE.DETAIL_TEXT_TYPE as DETAIL_TEXT_TYPE , PRD.TYPE as TYPE
					FROM 
			b_iblock B
			INNER JOIN b_lang L ON B.LID=L.LID
			INNER JOIN b_iblock_element BE ON BE.IBLOCK_ID = B.ID
			left join b_catalog_product as PRD on (PRD.ID = BE.ID)
			left join b_catalog_price as PRC_1 on (PRC_1.PRODUCT_ID = BE.ID and PRC_1.CATALOG_GROUP_ID = 1  and  ((PRC_1.QUANTITY_FROM <= 1 or PRC_1.QUANTITY_FROM IS NULL) and (PRC_1.QUANTITY_TO >= 1 or PRC_1.QUANTITY_TO IS NULL)))
		
					WHERE 1=1 
			AND (
			
				((((BE.IBLOCK_ID = '14'))))
				AND ((((BE.ACTIVE='Y'))))
				AND (((BE.ACTIVE_TO >= now() OR BE.ACTIVE_TO IS NULL) AND (BE.ACTIVE_FROM <= now() OR BE.ACTIVE_FROM IS NULL)))
				AND (
				
					(
					
						((((PRC_1.PRICE > '10000'))))
					)
				)
				AND (
				
					(( EXISTS (
						SELECT BSE.IBLOCK_ELEMENT_ID
						FROM b_iblock_section_element BSE
						
						INNER JOIN b_iblock_section BS ON BSE.IBLOCK_SECTION_ID = BS.ID
						
						WHERE BSE.IBLOCK_ELEMENT_ID = BE.ID
						AND ((BS.GLOBAL_ACTIVE = 'Y'))
						)))
				)
				AND (
				
					((((BE.ID > '360516'))))
				)
				AND ((( PRD.TYPE IS NULL OR NOT (PRD.TYPE = '3'))))
			)
			AND (((BE.WF_STATUS_ID=1 AND BE.WF_PARENT_ELEMENT_ID IS NULL)))
					
					 ORDER BY BE.ID asc 
					LIMIT 100\G

