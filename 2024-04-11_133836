#  10s+
# Tables
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_lang'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_lang`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_element'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_element`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_catalog_product'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_product`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_catalog_price'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_price`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_catalog_iblock'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_iblock`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_catalog_vat'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_vat`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_user'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_user`\G
# EXPLAIN /*!50100 PARTITIONS*/
SELECT COUNT('x') as C
						FROM 
			b_iblock B
			INNER JOIN b_lang L ON B.LID=L.LID
			INNER JOIN b_iblock_element BE ON BE.IBLOCK_ID = B.ID
			left join b_catalog_product as CAT_PR on (CAT_PR.ID = BE.ID)
			left join b_catalog_price as CAT_P1 on (CAT_P1.PRODUCT_ID = BE.ID and CAT_P1.CATALOG_GROUP_ID = 1)
			left join b_catalog_iblock as CAT_IB on ((CAT_PR.VAT_ID IS NULL or CAT_PR.VAT_ID = 0) and CAT_IB.IBLOCK_ID = BE.IBLOCK_ID)
			left join b_catalog_vat as CAT_VAT on (CAT_VAT.ID = CASE WHEN (CAT_PR.VAT_ID IS NULL OR CAT_PR.VAT_ID = 0) THEN CAT_IB.VAT_ID ELSE CAT_PR.VAT_ID END)
			left join b_catalog_price as PRC_FT on (PRC_FT.PRODUCT_ID = BE.ID)
			left join b_catalog_product as PRD on (PRD.ID = BE.ID)
			LEFT JOIN b_user U ON U.ID=BE.MODIFIED_BY
			LEFT JOIN b_user UL ON UL.ID=BE.WF_LOCKED_BY
			LEFT JOIN b_user UC ON UC.ID=BE.CREATED_BY
		
						WHERE 1=1 
			AND (
			
				((((BE.IBLOCK_ID = '14'))))
				AND ((((PRC_FT.PRICE > '0'))))
				AND ((((PRD.QUANTITY > '0'))))
			)
			AND (((BE.WF_STATUS_ID=1 AND BE.WF_PARENT_ELEMENT_ID IS NULL)))\G

# Query 3: 0.00 QPS, 0.00x concurrency, ID 0x4668E314BA15102A351C734552BEB900 at byte 94959834
# This item is included in the report because it matches --limit.
# Scores: V/M = 0.02
# Time range: 2024-03-31T22:02:15 to 2024-04-11T10:21:35
# Attribute    pct   total     min     max     avg     95%  stddev  median
# ============ === ======= ======= ======= ======= ======= ======= =======
# Count          9     665
# Exec time      8   1497s      2s      3s      2s      3s   196ms      2s
# Lock time      4   108ms   100us   371us   163us   247us    52us   159us
# Rows sent      0     665       1       1       1       1       0       1
# Rows examine   8 292.92M 447.63k 454.64k 451.05k 440.37k       0 440.37k
# Rows affecte   0       0       0       0       0       0       0       0
# Bytes sent     0  40.26k      62      62      62      62       0      62
# Query size     5 707.86k   1.06k   1.06k   1.06k   1.06k       0   1.06k
# String:
# Databases    torgsnhm_ruobo32
# Hosts        localhost
# Last errno   0
# Users        torgsnhm_ruobo32
# Query_time distribution
#   1us
#  10us
# 100us
#   1ms
#  10ms
# 100ms
#    1s  ################################################################
#  10s+
# Tables
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_lang'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_lang`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_element'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_element`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_14_index'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_14_index`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_group'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_group`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_element_right'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_element_right`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_iblock_right'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_iblock_right`\G
#    SHOW TABLE STATUS FROM `torgsnhm_ruobo32` LIKE 'b_user_access'\G
#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_user_access`\G
# EXPLAIN /*!50100 PARTITIONS*/
SELECT COUNT(DISTINCT BE.ID) as CNT
				FROM 
			b_iblock B
			INNER JOIN b_lang L ON B.LID=L.LID
			INNER JOIN b_iblock_element BE ON BE.IBLOCK_ID = B.ID
			INNER JOIN b_iblock_14_index FC on FC.ELEMENT_ID = BE.ID
		
				WHERE 1=1 
			AND (
			
				(FC.SECTION_ID = 2423 AND FC.FACET_ID = 1 AND FC.VALUE_NUM = 0 AND FC.VALUE in (0))
				AND ((((BE.IBLOCK_ID = '14'))))
				AND ((((BE.ACTIVE='Y'))))
				AND (((BE.ACTIVE_TO >= now() OR BE.ACTIVE_TO IS NULL) AND (BE.ACTIVE_FROM <= now() OR BE.ACTIVE_FROM IS NULL)))
			)
			AND (((BE.WF_STATUS_ID=1 AND BE.WF_PARENT_ELEMENT_ID IS NULL)))
			AND ((
				B.ID IN (
			SELECT IBLOCK_ID
			FROM b_iblock_group IBG
			WHERE IBG.GROUP_ID IN (2)
			AND IBG.PERMISSION >= 'R'
		
				AND (IBG.PERMISSION='X' OR B.ACTIVE='Y')
			)
				OR (B.RIGHTS_MODE = 'E' AND EXISTS (
				SELECT ER.ELEMENT_ID
				FROM b_iblock_element_right ER
				INNER JOIN b_iblock_right IBR ON IBR.ID = ER.RIGHT_ID
				INNER JOIN b_user_access UA ON UA.ACCESS_CODE = IBR.GROUP_CODE AND UA.USER_ID = 0
				WHERE ER.ELEMENT_ID = BE.ID
				AND IBR.OP_EREAD = 'Y'
				
			))
			))\G

