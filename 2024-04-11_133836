	`wd_seo_url`.`SEO_TEXT_3_TYPE` AS `SEO_TEXT_3_TYPE`,
	`wd_seo_url`.`SEO_TEXT_4` AS `SEO_TEXT_4`,
	`wd_seo_url`.`SEO_TEXT_4_TYPE` AS `SEO_TEXT_4_TYPE`,
	`wd_seo_url`.`SEO_TEXT_5` AS `SEO_TEXT_5`,
	`wd_seo_url`.`SEO_TEXT_5_TYPE` AS `SEO_TEXT_5_TYPE`,
	`wd_seo_url`.`SEO_TEXT_6` AS `SEO_TEXT_6`,
	`wd_seo_url`.`SEO_TEXT_6_TYPE` AS `SEO_TEXT_6_TYPE`,
	`wd_seo_url`.`SEO_TEXT_7` AS `SEO_TEXT_7`,
	`wd_seo_url`.`SEO_TEXT_7_TYPE` AS `SEO_TEXT_7_TYPE`,
	`wd_seo_url`.`SEO_TEXT_8` AS `SEO_TEXT_8`,
	`wd_seo_url`.`SEO_TEXT_8_TYPE` AS `SEO_TEXT_8_TYPE`,
	`wd_seo_url`.`SEO_TEXT_9` AS `SEO_TEXT_9`,
	`wd_seo_url`.`SEO_TEXT_9_TYPE` AS `SEO_TEXT_9_TYPE`,
	`wd_seo_url_rule`.`ID` AS `RULE_ID`,
	`wd_seo_url_rule`.`NAME` AS `RULE_NAME`,
	`wd_seo_url_rule`.`ACTIVE` AS `RULE_ACTIVE`,
	`wd_seo_url_rule`.`SECTION_ID` AS `RULE_SECTION_ID`,
	`wd_seo_url_profile`.`ACTIVE` AS `PROFILE_ACTIVE`,
	`wd_seo_url_profile`.`SITE_ID` AS `PROFILE_SITE_ID`,
	`wd_seo_url_rule`.`PROFILE_ID` AS `PROFILE_ID`,
	`wd_seo_url_profile`.`IBLOCK_ID` AS `PROFILE_IBLOCK_ID`,
	`wd_seo_url_profile`.`FILTER_TYPE` AS `PROFILE_FILTER_TYPE`,
	`wd_seo_url_profile`.`ID` AS `UALIAS_0`
FROM `wd_seo_url` `wd_seo_url` 
LEFT JOIN `wd_seo_smartfilter_rule` `wd_seo_url_rule` ON `wd_seo_url_rule`.`ID` = `wd_seo_url`.`PARENT_ID`
LEFT JOIN `wd_seo_smartfilter_profile` `wd_seo_url_profile` ON `wd_seo_url_profile`.`ID` = `wd_seo_url_rule`.`PROFILE_ID`
WHERE (`wd_seo_url`.`ACTIVE` IS NULL OR `wd_seo_url`.`ACTIVE` <> 'N')
AND (
	(
		`wd_seo_url_rule`.`ACTIVE` = 'Y'
	)
	OR (
		(`wd_seo_url_rule`.`ACTIVE` IS NULL OR `wd_seo_url_rule`.`ACTIVE` = '')
	)
)
AND (
	(
		`wd_seo_url_profile`.`ACTIVE` = 'Y'
	)
	OR (
		(`wd_seo_url_profile`.`ACTIVE` IS NULL OR `wd_seo_url_profile`.`ACTIVE` = '')
	)
)
AND (
	(
		`wd_seo_url_profile`.`SITE_ID` = 's1'
	)
	OR (
		`wd_seo_url`.`SITE_ID` = 's1'
	)
)
AND (
	(
		`wd_seo_url`.`URL_REAL` = '/catalog/teplovoe_oborudovanie/grili_kontaktnye/399313/'
	)
	OR (
		`wd_seo_url`.`URL_SEO` = '/catalog/teplovoe_oborudovanie/grili_kontaktnye/399313/'
	)
)
ORDER BY `ID` ASC
LIMIT 0, 1\G

