#    SHOW CREATE TABLE `torgsnhm_ruobo32`.`b_catalog_price`\G
# EXPLAIN /*!50100 PARTITIONS*/
SELECT  BE.IBLOCK_ID as IBLOCK_ID,BE.ID as ID,BE.DETAIL_PICTURE as DETAIL_PICTURE,B.DETAIL_PAGE_URL as DETAIL_PAGE_URL,BE.IBLOCK_SECTION_ID as IBLOCK_SECTION_ID,BE.NAME as NAME,BE.DETAIL_TEXT as DETAIL_TEXT,L.DIR as LANG_DIR,BE.CODE as CODE,BE.XML_ID as EXTERNAL_ID,B.IBLOCK_TYPE_ID as IBLOCK_TYPE_ID,B.CODE as IBLOCK_CODE,B.XML_ID as IBLOCK_EXTERNAL_ID,B.LID as LID,BE.DETAIL_TEXT_TYPE as DETAIL_TEXT_TYPE , PRD.TYPE as TYPE
					FROM 
			b_iblock B
			INNER JOIN b_lang L ON B.LID=L.LID
			INNER JOIN b_iblock_element BE ON BE.IBLOCK_ID = B.ID
			 INNER JOIN (
					SELECT DISTINCT BSE.IBLOCK_ELEMENT_ID
					FROM b_iblock_section_element BSE
					
					INNER JOIN b_iblock_section BS ON BSE.IBLOCK_SECTION_ID = BS.ID
					
					WHERE ((BS.GLOBAL_ACTIVE = 'Y') AND (( NOT (BS.LEFT_MARGIN >= 2 AND BS.RIGHT_MARGIN <= 5)
AND  NOT (BS.LEFT_MARGIN >= 80 AND BS.RIGHT_MARGIN <= 81)
AND  NOT (BS.LEFT_MARGIN >= 348 AND BS.RIGHT_MARGIN <= 349)
AND  NOT (BS.LEFT_MARGIN >= 398 AND BS.RIGHT_MARGIN <= 401)
AND  NOT (BS.LEFT_MARGIN >= 466 AND BS.RIGHT_MARGIN <= 467)
AND  NOT (BS.LEFT_MARGIN >= 475 AND BS.RIGHT_MARGIN <= 498)
AND  NOT (BS.LEFT_MARGIN >= 500 AND BS.RIGHT_MARGIN <= 521)
AND  NOT (BS.LEFT_MARGIN >= 664 AND BS.RIGHT_MARGIN <= 667)
AND  NOT (BS.LEFT_MARGIN >= 786 AND BS.RIGHT_MARGIN <= 787)
AND  NOT (BS.LEFT_MARGIN >= 808 AND BS.RIGHT_MARGIN <= 809)
AND  NOT (BS.LEFT_MARGIN >= 820 AND BS.RIGHT_MARGIN <= 823)
AND  NOT (BS.LEFT_MARGIN >= 904 AND BS.RIGHT_MARGIN <= 905)
AND  NOT (BS.LEFT_MARGIN >= 942 AND BS.RIGHT_MARGIN <= 945)
AND  NOT (BS.LEFT_MARGIN >= 973 AND BS.RIGHT_MARGIN <= 974)
AND  NOT (BS.LEFT_MARGIN >= 975 AND BS.RIGHT_MARGIN <= 1080)
AND  NOT (BS.LEFT_MARGIN >= 1084 AND BS.RIGHT_MARGIN <= 1085)
AND  NOT (BS.LEFT_MARGIN >= 1102 AND BS.RIGHT_MARGIN <= 1103)
AND  NOT (BS.LEFT_MARGIN >= 1125 AND BS.RIGHT_MARGIN <= 1180)
AND  NOT (BS.LEFT_MARGIN >= 1181 AND BS.RIGHT_MARGIN <= 1182)
AND  NOT (BS.LEFT_MARGIN >= 1196 AND BS.RIGHT_MARGIN <= 1205)
AND  NOT (BS.LEFT_MARGIN >= 1346 AND BS.RIGHT_MARGIN <= 1347)
AND  NOT (BS.LEFT_MARGIN >= 1360 AND BS.RIGHT_MARGIN <= 1361)
AND  NOT (BS.LEFT_MARGIN >= 1406 AND BS.RIGHT_MARGIN <= 1407)
AND  NOT (BS.LEFT_MARGIN >= 1458 AND BS.RIGHT_MARGIN <= 1459)
)))
					) BES ON BES.IBLOCK_ELEMENT_ID = BE.ID
			left join b_catalog_product as PRD on (PRD.ID = BE.ID)
			left join b_catalog_store_product as WHS_3 on (WHS_3.PRODUCT_ID = BE.ID and WHS_3.STORE_ID = 3)
			left join b_catalog_price as PRC_1 on (PRC_1.PRODUCT_ID = BE.ID and PRC_1.CATALOG_GROUP_ID = 1  and  ((PRC_1.QUANTITY_FROM <= 1 or PRC_1.QUANTITY_FROM IS NULL) and (PRC_1.QUANTITY_TO >= 1 or PRC_1.QUANTITY_TO IS NULL)))
		
					WHERE 1=1 
			AND (
			
				((((BE.IBLOCK_ID = '14'))))
				AND ((((BE.ACTIVE='Y'))))
				AND (((BE.ACTIVE_TO >= now() OR BE.ACTIVE_TO IS NULL) AND (BE.ACTIVE_FROM <= now() OR BE.ACTIVE_FROM IS NULL)))
				AND (
				
					(
					
						((((WHS_3.AMOUNT >= '1'))))
					)
					AND (
					
						((((PRC_1.PRICE >= '10000'))))
					)
				)
				AND (
				
					((((BE.ID > '351093'))))
				)
				AND ((( PRD.TYPE IS NULL OR NOT (PRD.TYPE = '3'))))
			)
			AND (((BE.WF_STATUS_ID=1 AND BE.WF_PARENT_ELEMENT_ID IS NULL)))
					
					 ORDER BY BE.ID asc 
					LIMIT 100\G

